<% include partials/header%>

<div class="container-fluid">
  <div class="row">
     <%videos.forEach((video) => {%>
     <div class="col-lg-4">
      <p><h4><%=video.title%></h4></p>
     <iframe width="1000" height="900" class="img-thumbnail" src="https://www.youtube-nocookie.com/embed/<%=video.url%>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe> 
    </div>
  <%})%>
  </div>
</div>

<% include partials/footer%>